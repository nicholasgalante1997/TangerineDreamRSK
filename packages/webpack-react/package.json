{
  "name": "tangerine-dream-react-starter-kit",
  "version": "1.0.0",
  "description": "A react app made with the @tangerinedream/react starter kit.",
  "scripts": {
    "start": "npx serve build",
    "test": "jest --passWithNoTests",
    "cp:assets": "cp -R src/styles/*.css src/styles/*.ttf assets/* build/",
    "clean:build": "rm -rf build",
    "storybook": "storybook dev -p 6006",
    "dev": "webpack serve --config webpack/development.webpack.js",
    "build-storybook": "storybook build",
    "format": "prettier --write src workers webpack",
    "lint": "eslint --fix \"{src,workers}/**/*.{ts,tsx,js}\" ",
    "pkg:js": "webpack --config webpack/browser.webpack.js",
    "pkg:ts-script": "webpack --config webpack/server.webpack.js",
    "pkg:html": "node .build-process/build-app.js",
    "clean:tmp": "rm -rf .build-process",
    "cp:workers": "cp -R workers/* build",
    "build": "npm-run-all clean:build pkg:ts-script pkg:js pkg:html clean:tmp cp:assets cp:workers"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "pino": "^8.14.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.22.5",
    "@storybook/addon-essentials": "^7.0.24",
    "@storybook/addon-interactions": "^7.0.24",
    "@storybook/addon-links": "^7.0.24",
    "@storybook/blocks": "^7.0.24",
    "@storybook/react": "^7.0.24",
    "@storybook/react-webpack5": "^7.0.24",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/node": "^20.5.8",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "babel-loader": "^9.1.2",
    "bundle-stats-webpack-plugin": "^4.4.2",
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "json-loader": "^0.5.7",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^10.2.0",
    "prettier": "^2.8.8",
    "storybook": "^7.0.24",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  }
}